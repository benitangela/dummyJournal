<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%= startingContent %></p>

<% newPost.forEach(function(newContent) { %>
  <h1><%= newContent.title %></h1>
  <p>
    <%= newContent.body.substring(0, 100) + " ..."  %>
    <a href="/posts/<%= newContent._id %>">Read More</a>
  </p>
<% }); %>

<%- include('partials/footer'); -%>
